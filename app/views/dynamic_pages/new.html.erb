<% provide(:h1, "Create a New Page") %>

<%= form_for @dynamic_page, url: { action: :create, slug: @dynamic_page.slug } do |f| %>
  <%= render 'dynamic_pages/form', fields: f %>

  <div class="actions">
      <%= f.submit f.object.new_record? ? "Submit" : "Update", class: "btn btn-primary" %>
    </div>
<% end %>